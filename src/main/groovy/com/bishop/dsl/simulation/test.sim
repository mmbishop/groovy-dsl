simscript {

    malfunction "m1" variable "sv02" offset -5

    when (altitude >= 10000) {
        sv01 = 0
    }

    whenever (sv02 > 1) until (altitude >= 20000) {
        sv03 = 0
    }

    if (altitude == 25000) {
        insert "m1"
    }
    if (altitude == 30000) {
        remove "m1"
    }

}